{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "allowJs": false,
    "strictNullChecks": true
  },
  "include": ["src/**/*"],
  "exclude": ["**/*.test.ts"]
}
